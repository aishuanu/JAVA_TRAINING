import java.util.*;
public class Main
{
	public static void main(String[] args) {
	        Scanner sc=new Scanner(System.in);
	        int n=sc.nextInt();
	        if(n==0)
	        {
	                System.out.println("zero");
	        }
	        else{
	                String res=n>0 ?"positive":"negative";
	                System.out.print(res);
	        }
	}
}
