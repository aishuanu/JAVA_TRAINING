import java.util.*;
public class Main
{
	public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
         String str=sc.nextLine();
         for(int i=0;i<str.length();i++)
         {
                 char c=str.charAt(i);
                  System.out.println(c+" "+(int)c);
         }
	}
}
